<div>ログイン</div>

<%= form_with model:@user, url: user_session_path, local: true do |f| %>
  <div class="form-wrap">
  <div class="form-text">
    <label>Eメール</label>
  </div>
  <%= f.text_field :email %>
</div>

<div class="form-wrap">
  <div class="form-text">
    <label>パスワード</label>
  </div>
  <%= f.text_field :password %>
</div>

<div class="form_submit">
  <%= f.submit "ログイン"%>
</div>
<% end%>